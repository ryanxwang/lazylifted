{
    "version": "0.2.0",
    "configurations": [
        {
            "type": "lldb",
            "request": "launch",
            "name": "Debug unit tests in library 'lazylifted'",
            "cargo": {
                "args": [
                    "test",
                    "--no-run",
                    "--lib",
                    "--package=lazylifted"
                ],
                "filter": {
                    "name": "lazylifted",
                    "kind": "lib"
                }
            },
            "args": [],
            "cwd": "${workspaceFolder}"
        },
        {
            "type": "lldb",
            "request": "launch",
            "name": "Debug executable 'planner' for blocksworld",
            "cargo": {
                "args": [
                    "build",
                    "--bin=planner",
                    "--package=lazylifted"
                ],
                "filter": {
                    "name": "planner",
                    "kind": "bin"
                }
            },
            "args": [
                "${workspaceFolder}/benchmarks/ipc23-learning/blocksworld/domain.pddl",
                "${workspaceFolder}/benchmarks/ipc23-learning/blocksworld/training/easy/p01.pddl",
                "partial-action-search",
                "--heuristic",
                "wl-palg",
                "--model",
                "${workspaceFolder}/trained_models/wl-palg-lambdamart-blocksworld",
            ],
            "cwd": "${workspaceFolder}"
        },
        {
            "type": "lldb",
            "request": "launch",
            "name": "Debug executable 'planner' for spanner",
            "cargo": {
                "args": [
                    "build",
                    "--bin=planner",
                    "--package=lazylifted"
                ],
                "filter": {
                    "name": "planner",
                    "kind": "bin"
                }
            },
            "args": [
                "--model",
                "${workspaceFolder}/trained_models/wl-sclg-gpr-spanner",
                "--model-config",
                "${workspaceFolder}/experiments/models/wl-sclg-gpr.toml",
                "${workspaceFolder}/benchmarks/ipc23-learning/spanner/domain.pddl",
                "${workspaceFolder}/benchmarks/ipc23-learning/spanner/testing/easy/p10.pddl",
                "partial-action-search",
                "--heuristic",
                "wl-palg",
            ],
            "cwd": "${workspaceFolder}"
        },
        {
            "type": "lldb",
            "request": "launch",
            "name": "Debug unit tests in executable 'planner'",
            "cargo": {
                "args": [
                    "test",
                    "--no-run",
                    "--bin=planner",
                    "--package=lazylifted"
                ],
                "filter": {
                    "name": "planner",
                    "kind": "bin"
                }
            },
            "args": [],
            "cwd": "${workspaceFolder}"
        },
        {
            "type": "lldb",
            "request": "launch",
            "name": "Debug executable 'trainer' on blocksworld",
            "cargo": {
                "args": [
                    "build",
                    "--bin=trainer",
                    "--package=lazylifted"
                ],
                "filter": {
                    "name": "trainer",
                    "kind": "bin"
                }
            },
            "args": [
                "--data",
                "${workspaceFolder}/experiments/ipc23-learning/blocksworld.toml",
                "--model",
                "${workspaceFolder}/experiments/models/wl-palg-lambdamart.toml"
            ],
            "cwd": "${workspaceFolder}"
        },
        {
            "type": "lldb",
            "request": "launch",
            "name": "Debug unit tests in executable 'trainer'",
            "cargo": {
                "args": [
                    "test",
                    "--no-run",
                    "--bin=trainer",
                    "--package=lazylifted"
                ],
                "filter": {
                    "name": "trainer",
                    "kind": "bin"
                }
            },
            "args": [],
            "cwd": "${workspaceFolder}"
        }
    ]
}